{
  "multipart": [
    {
      "when": {
        "horizontal": "true"
      },
      "apply": {
        "model": "biomemakeover:block/cactus_stalk",
        "uvlock": true
      }
    },
    {
      "when": {
        "horizontal": "false"
      },
      "apply": {
        "model": "biomemakeover:block/cactus_regular",
        "uvlock": true
      }
    },
    {
      "when": {
        "north": "true"
      },
      "apply": {
        "model": "biomemakeover:block/cactus_stalk_part",
        "uvlock": true
      }
    },
    {
      "when": {
        "east": "true"
      },
      "apply": {
        "model": "biomemakeover:block/cactus_stalk_part",
        "y": 90,
        "uvlock": true
      }
    },
    {
      "when": {
        "south": "true"
      },
      "apply": {
        "model": "biomemakeover:block/cactus_stalk_part",
        "y": 180,
        "uvlock": true
      }
    },
    {
      "when": {
        "west": "true"
      },
      "apply": {
        "model": "biomemakeover:block/cactus_stalk_part",
        "y": 270,
        "uvlock": true
      }
    }
  ]
}