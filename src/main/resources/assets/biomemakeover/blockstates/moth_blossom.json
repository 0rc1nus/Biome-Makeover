{
  "multipart": [
    {
      "when": {
        "blossom": "up"
      },
      "apply": [
        {
          "model": "biomemakeover:block/moth_blossom",
          "x": 180,
          "uvlock": false
        }
      ]
    },
    {
      "when": {
        "blossom": "west"
      },
      "apply": [
        {
          "model": "biomemakeover:block/moth_blossom_wall",
          "y": 180,
          "uvlock": false
        }
      ]
    },
    {
      "when": {
        "blossom": "down"
      },
      "apply": [
        {
          "model": "biomemakeover:block/moth_blossom"
        }
      ]
    },
    {
      "when": {
        "blossom": "east"
      },
      "apply": [
        {
          "model": "biomemakeover:block/moth_blossom_wall",
          "uvlock": false
        }
      ]
    },
    {
      "when": {
        "blossom": "south"
      },
      "apply": [
        {
          "model": "biomemakeover:block/moth_blossom",
          "x": 90,
          "uvlock": false
        }
      ]
    },



    {
      "when": {
        "blossom": "north"
      },
      "apply": [
        {
          "x": 270,
          "model": "biomemakeover:block/moth_blossom"
        }
      ]
    },
    {
      "when": {
        "up": "true"
      },
      "apply": [
        {
          "model": "biomemakeover:block/itching_ivy",
          "x": 270,
          "uvlock": true
        }
      ]
    },
    {
      "when": {
        "west": "true"
      },
      "apply": [
        {
          "model": "biomemakeover:block/itching_ivy",
          "y": 270,
          "uvlock": true
        }
      ]
    },
    {
      "when": {
        "down": "true"
      },
      "apply": [
        {
          "model": "biomemakeover:block/itching_ivy",
          "x": 90,
          "uvlock": true
        }
      ]
    },
    {
      "when": {
        "east": "true"
      },
      "apply": [
        {
          "model": "biomemakeover:block/itching_ivy",
          "y": 90,
          "uvlock": true
        }
      ]
    },
    {
      "when": {
        "south": "true"
      },
      "apply": [
        {
          "model": "biomemakeover:block/itching_ivy",
          "y": 180,
          "uvlock": true
        }
      ]
    },
    {
      "when": {
        "north": "true"
      },
      "apply": [
        {
          "model": "biomemakeover:block/itching_ivy"
        }
      ]
    }
  ]
}